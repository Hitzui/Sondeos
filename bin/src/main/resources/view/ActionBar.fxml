<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="506.0" prefWidth="197.0" spacing="10.0" style="-fx-background-color: -fx-dark;" stylesheets="@../css/application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="imagenEmpresa" fitHeight="150.0" fitWidth="175.0" pickOnBounds="true">
         <image>
            <Image url="@../image/company_name.png" />
         </image>
      </ImageView>
      <Button fx:id="btnTipoSuelo" mnemonicParsing="false" prefHeight="39.0" prefWidth="175.0" styleClass="button-hamburger" text="Tipos de Suelo" />
      <Button fx:id="btnIngresarValores" mnemonicParsing="false" prefHeight="39.0" prefWidth="175.0" styleClass="button-hamburger" text="Ingreso Valores" />
      <Button fx:id="btnClientes" mnemonicParsing="false" prefHeight="39.0" prefWidth="175.0" styleClass="button-hamburger" text="Clientes" />
      <Button fx:id="btnConfiguration" alignment="CENTER" mnemonicParsing="false" prefHeight="39.0" prefWidth="175.0" styleClass="button-hamburger" text="Configuracion" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
