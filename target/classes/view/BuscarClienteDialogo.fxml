<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>


<StackPane xmlns="http://javafx.com/javafx/8.0.181" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane prefHeight="400.0" prefWidth="600.0" StackPane.alignment="CENTER">
            <children>
                <TableView fx:id="tableClientes" layoutX="14.0" layoutY="14.0" prefHeight="353.0" prefWidth="600.0"
                           AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn fx:id="colCodigo" prefWidth="108.0" text="Codigo"/>
                        <TableColumn fx:id="colCliente" prefWidth="191.0" text="Cliente"/>
                        <TableColumn fx:id="colProyecto" prefWidth="180.0" text="Proyecto"/>
                        <TableColumn fx:id="colOperador" prefWidth="150.0" text="Operador"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
                <Button fx:id="btnCancelar" layoutX="513.0" layoutY="361.0" mnemonicParsing="false" text="Cancelar"
                        AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="35.0"/>
                <Button fx:id="btnSeleccionar" layoutX="340.0" layoutY="361.0" mnemonicParsing="false"
                        text="Seleccionar" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="180.0"/>
                <TextField fx:id="txtFiltrar" layoutX="41.0" layoutY="361.0" prefHeight="25.0" prefWidth="226.0"
                           AnchorPane.bottomAnchor="15.0" AnchorPane.leftAnchor="40.0"/>
            </children>
        </AnchorPane>
    </children>
</StackPane>
